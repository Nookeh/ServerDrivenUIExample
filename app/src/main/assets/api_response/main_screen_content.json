{
  "components": [
    {
      "id": "toolbar",
      "type": "TOOLBAR",
      "content": {
        "title": "Server-Driven UI Example"
      }
    },
    {
      "id": "spacer1",
      "type": "VERTICAL_SPACE",
      "properties": {
        "height": 24
      }
    },
    {
      "id": "button_static_screen",
      "type": "TEXT_BUTTON",
      "content": {
        "text": "Go To Static Screen"
      },
      "properties": {
        "margin_start": 24,
        "margin_end": 24,
        "color": "#000000",
        "text_color": "#ffffff"
      },
      "actions": {
        "on_click": {
          "case": "DEEP_LINK",
          "data": {
            "url": "sdui://com.example.server_drivenuiexample.static_screen"
          }
        }
      }
    },
    {
      "id": "button_dialog",
      "type": "TEXT_BUTTON",
      "content": {
        "text": "Open Popup dialog"
      },
      "properties": {
        "margin_start": 24,
        "margin_end": 24,
        "color": "#000000",
        "text_color": "#ffffff"
      },
      "actions": {
        "on_click": {
          "case": "DEEP_LINK",
          "data": {
            "url": "sdui://com.example.server_drivenuiexample.main/show_caution_dialog?blocking=true"
          }
        }
      }
    },
    {
      "id": "button_grid_screen",
      "type": "TEXT_BUTTON",
      "content": {
        "text": "Open A Screen With A Complex Content"
      },
      "properties": {
        "margin_start": 24,
        "margin_end": 24,
        "color": "#000000",
        "text_color": "#ffffff"
      },
      "actions": {
        "on_click": {
          "case": "DEEP_LINK",
          "data": {
            "url": "sdui://com.example.server_drivenuiexample.complex_screen?grid_span_count=3&json=complex_screen_content.json"
          }
        }
      }
    },
    {
      "id": "button_grid_screen_2",
      "type": "TEXT_BUTTON",
      "content": {
        "text": "Open A Screen With A Complex Content 2"
      },
      "properties": {
        "margin_start": 24,
        "margin_end": 24,
        "color": "#000000",
        "text_color": "#ffffff"
      },
      "actions": {
        "on_click": {
          "case": "DEEP_LINK",
          "data": {
            "url": "sdui://com.example.server_drivenuiexample.complex_screen?grid_span_count=3&json=complex_screen_content_2.json"
          }
        }
      }
    },
    {
      "id": "button_grid_screen_3",
      "type": "TEXT_BUTTON",
      "content": {
        "text": "HESHAM BUTTON"
      },
      "properties": {
        "margin_start": 24,
        "margin_end": 24,
        "color": "#000000",
        "text_color": "#ffffff"
      },
      "actions": {
        "on_click": {
          "case": "DEEP_LINK",
          "data": {
            "url": "sdui://com.example.server_drivenuiexample.complex_screen?grid_span_count=3&json=complex_screen_content_2.json"
          }
        }
      }
    }
  ]
}